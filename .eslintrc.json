{
  "rules": {
    "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
    ],
    "quotes": [
        2,
        "single"
    ],
    "linebreak-style": [
        2,
        "unix"
    ],
    "semi": [
        2,
        "always"
    ],
    "no-console": [
        0
    ],
    "no-unused-vars": [
      1
    ],
    "valid-jsdoc": [
      1,
      {
        "requireReturn": false,
        "requireReturnDescription": false
      }
    ]
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "mocha": true
  },
  "extends": "eslint:recommended"
}
